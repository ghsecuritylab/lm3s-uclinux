config interface lo
	option ifname   lo
	option proto    static
	option ipaddr   127.0.0.1
	option netmask  255.0.0.0

config interface lan
	option ifname   eth0
	option proto    static
	option ipaddr   10.65.100.205
	option netmask  255.255.255.0
	option macaddr  4c:22:d0:b8:78:ae
	option dns      8.8.8.8

config interface wan
	option ifname ppp0

config default route
	option ifname   eth0
	option target   default
	option gateway  10.65.100.1
	option metric   100